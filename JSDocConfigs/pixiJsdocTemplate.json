{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["./src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "applicationName": "JSDoc Template Examples",
    "linenums": true
  },
  "opts": {
    "destination": "build/pixiJsdocTemplate",
    "encoding": "utf8",
    "verbose": true,
    "recurse": true,
    "readme": "README.md",
    "template": "./node_modules/@pixi/jsdoc-template"
  }
}
