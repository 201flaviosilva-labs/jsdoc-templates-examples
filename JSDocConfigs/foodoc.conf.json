{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["./src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "systemName": "JSDoc Template Examples",
    "systemSummary": "Just testing some JSDoc templates",
    "includeDate": true,
    "dateFormat": "DD MM YYYY",
    "linenums": true,
    "showTableOfContents": true,
    "showAccessFilter": true,
    "collapseSymbols": true,
    "methodHeadingReturns": true,
    "search": true
  },
  "opts": {
    "destination": "build/foodoc",
    "encoding": "utf8",
    "verbose": true,
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/foodoc/template"
  }
}
