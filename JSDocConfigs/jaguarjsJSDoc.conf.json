{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["./src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "applicationName": "JSDoc Templates Example"
  },
  "opts": {
    "destination": "build/jaguarjsJSDoc",
    "encoding": "utf8",
    "verbose": true,
    "recurse": true,
    "readme": "README.md",
    "private": false,
    "lenient": true,
    "template": "./node_modules/jaguarjs-jsdoc"
  }
}
